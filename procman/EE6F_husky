cmd "01_core" {
    exec = "roscore";
    host = "localhost";
}
cmd "02_EE6F" {
    exec = "roslaunch real_to_sim_env gazebo_world.launch world:=EE6F";
    host = "localhost";
}
cmd "03_husky" {
    exec = "roslaunch subt_example one_robot.launch";
    host = "localhost";
}


script "open_EE6F_husky" {
    start cmd "01_core" wait "running";
    wait ms 2000;
    start cmd "02_EE6F" wait "running";
    wait ms 2000;
}